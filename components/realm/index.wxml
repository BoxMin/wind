<view class="container">
    <view class="sku-privew">
        <image class="sku-img"> </image>
        <view class="sku-description">
            <text class="title">测试SKU</text>
            <view>
                <view class="price-row">
                    <l-price autofix
                            value="777"
                            color="#157658"
                            value-size="28"></l-price>
                    <l-price deleted
                            value="999"
                            l-class="discount-price"
                            color="#999999"
                            size="26"></l-price>
                </view>
                <view class="sku-pending">
                    <text>请选择：</text>
                </view>
            </view>
        </view>
    </view>

    <view class="hr"></view>
    <block wx:for="{{fences}}" wx:key="{{index}}">
        <s-fence x="{{index}}" bind:cellTap="onCellTap" fence="{{item}}"></s-fence>
    </block>
    <view class="counter-container">
        <!--        <l-counter></l-counter>-->
    </view>
    <view></view>
</view>
